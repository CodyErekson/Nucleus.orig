{
    "name": "chillem/nucleus",
    "description": "Family management system",
    "keywords": ["family", "chores", "activities", "manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Erekson",
            "email": "cody.erekson@gmail.com",
            "homepage": "http://codyerekson.me/"
        }
    ],
    "require": {
        "php": "^7.2",
        "slim/slim": "3.*@dev",
        "slimphp-api/slim-eloquent": "0.1.*",
        "slimphp-api/slim-phinx": "0.1.*",
        "slimphp-api/slim-mvc": "0.1.*",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^2.4.0",
        "tuupola/slim-jwt-auth": "^2.3",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {"Nucleus\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"NucleusTest\\": "tests/phpunit/"}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
